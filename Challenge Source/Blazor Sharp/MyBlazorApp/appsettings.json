{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://0.0.0.0:8080",
        "Certificate": {
          "Path": "/certs/fullchain.pem",
          "KeyPath": "/certs/privkey.pem"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
